{
  "name": "@rwx/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "@rwx/typescript-live-types": "./src/index.ts",
      "default": "./dist/index.cjs"
    },
    "./client": {
      "@rwx/typescript-live-types": "./src/client.ts",
      "default": "./dist/client.cjs"
    },
    "./schema": {
      "@rwx/typescript-live-types": "./src/schema.ts",
      "default": "./dist/schema.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "push": "pnpm with-env drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@vercel/postgres": "^0.9.0",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "pg": "^8.13.0",
    "postgres": "^3.4.4",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@rwx/eslint-config": "workspace:*",
    "@rwx/prettier-config": "workspace:*",
    "@rwx/tsconfig": "workspace:*",
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.24.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@rwx/prettier-config"
}
