{
  "name": "@rwx/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "@rwx/typescript-live-types": "./src/index.ts",
      "import": "./dist/index.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
