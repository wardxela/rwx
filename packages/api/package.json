    "openapi-fetch": "^0.12.2"
  },
  "devDependencies": {
    "@rwx/eslint-config": "workspace:*",
    "@rwx/prettier-config": "workspace:*",
    "@rwx/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "openapi-typescript": "^7.4.1",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@rwx/prettier-config"
}
