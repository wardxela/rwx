  "dependencies": {
    "@rwx/auth": "workspace:*",
    "@rwx/db": "workspace:*",
    "@trpc/server": "catalog:",
    "superjson": "2.2.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@rwx/eslint-config": "workspace:*",
    "@rwx/prettier-config": "workspace:*",
    "@rwx/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@rwx/prettier-config"
}
