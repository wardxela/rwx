    "@rwx/tsconfig": "workspace:*",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
